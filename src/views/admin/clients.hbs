<h4 class="text-center">CLIENTS</h4>

<div class="container p-4">
  <div class="row">
    <table class="table">
          <thead class="thead-dark">
            <tr>
              <th>ID</th>
              <th scope="col">Email</th>

            </tr>
          </thead>

          <tbody>
            {{#each clients}}
              
              <tr>
                <td>{{id}}</td>
                <td>{{email}}</td>
              </tr>
              
            {{/each}}
          </tbody>
    </table>
  </div>
</div>